                            owl:onProperty      :hasSpiciness ;
                            owl:someValuesFrom  :Mild
                          ] ;
        owl:disjointWith  :FourCheesesTopping , :ParmesanTopping , :MozzarellaTopping , :GoatsCheeseTopping .

