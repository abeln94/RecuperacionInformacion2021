<http://www.co-ode.org/ontologies/pizza/pizza.owl>
        a                owl:Ontology ;
        rdfs:comment     "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
        owl:versionInfo  "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en , "version 1.5" , "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en .

:SpicyPizza  a               owl:Class ;
        rdfs:comment         "Any pizza that has a spicy topping is a SpicyPizza"@en ;
        rdfs:label           "PizzaTemperada"@pt ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( :Pizza
                                                     [ a                   owl:Restriction ;
                                                       owl:onProperty      :hasTopping ;
                                                       owl:someValuesFrom  :SpicyTopping
                                                     ]
                                                   )
                             ] .

:DeepPanBase  a           owl:Class ;
        rdfs:label        "BaseEspessa"@pt ;
        rdfs:subClassOf   :PizzaBase ;
        owl:disjointWith  :ThinAndCrispyBase .

:Food   a                owl:Class ;
        rdfs:subClassOf  :DomainConcept .

:AnchoviesTopping  a      owl:Class ;
        rdfs:label        "CoberturaDeAnchovies"@pt ;
        rdfs:subClassOf   :FishTopping ;
        owl:disjointWith  :PrawnsTopping , :MixedSeafoodTopping .

:CaperTopping  a          owl:Class ;
        rdfs:label        "CoberturaDeCaper"@pt ;
        rdfs:subClassOf   :VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasSpiciness ;
                            owl:someValuesFrom  :Mild
                          ] ;
        owl:disjointWith  :MushroomTopping , :ArtichokeTopping , :LeekTopping , :SpinachTopping , :PetitPoisTopping , :GarlicTopping , :TomatoTopping , :OliveTopping , :AsparagusTopping , :OnionTopping , :RocketTopping , :PepperTopping .

:OnionTopping  a          owl:Class ;
        rdfs:label        "CoberturaDeCebola"@pt ;
        rdfs:subClassOf   :VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasSpiciness ;
                            owl:someValuesFrom  :Medium
                          ] ;
        owl:disjointWith  :PetitPoisTopping , :MushroomTopping , :RocketTopping , :ArtichokeTopping , :PepperTopping , :SpinachTopping , :OliveTopping , :LeekTopping , :CaperTopping , :GarlicTopping , :TomatoTopping , :AsparagusTopping .

:FishTopping  a           owl:Class ;
        rdfs:label        "CoberturaDePeixe"@pt ;
        rdfs:subClassOf   :PizzaTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasSpiciness ;
                            owl:someValuesFrom  :Mild
                          ] ;
        owl:disjointWith  :MeatTopping , :FruitTopping , :VegetableTopping , :NutTopping , :SauceTopping , :CheeseTopping , :HerbSpiceTopping .

:CheeseyVegetableTopping
        a                owl:Class ;
        rdfs:comment     "This class will be inconsistent. This is because we have given it 2 disjoint parents, which means it could never have any members (as nothing can simultaneously be a CheeseTopping and a VegetableTopping). NB Called ProbeInconsistentTopping in the ProtegeOWL Tutorial."@en ;
        rdfs:label       "CoberturaDeQueijoComVegetais"@pt ;
        rdfs:subClassOf  :VegetableTopping , :CheeseTopping .

