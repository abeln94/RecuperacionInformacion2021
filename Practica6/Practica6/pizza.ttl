        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :MozzarellaTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :ArtichokeTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :HamTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :OliveTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :AnchoviesTopping
                          ] ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasTopping ;
                            owl:someValuesFrom  :TomatoTopping
                          ] ;
        owl:disjointWith  :Soho , :Giardiniera , :PrinceCarlo , :AmericanHot , :Margherita , :FourSeasons , :Parmense , :Capricciosa , :UnclosedPizza , :Veneziana , :Rosa , :Mushroom , :American , :FruttiDiMare , :Fiorentina , :QuattroFormaggi , :SloppyGiuseppe , :LaReine , :Napoletana , :Cajun , :PolloAdAstra , :Caprina .

:JalapenoPepperTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeJalapeno"@pt ;
        rdfs:subClassOf   :PepperTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasSpiciness ;
                            owl:someValuesFrom  :Hot
                          ] ;
        owl:disjointWith  :SweetPepperTopping , :PeperonataTopping , :GreenPepperTopping .

:hasTopping  a              owl:InverseFunctionalProperty , owl:ObjectProperty ;
        rdfs:comment        "Note that hasTopping is inverse functional because isToppingOf is functional"@en ;
        rdfs:domain         :Pizza ;
        rdfs:range          :PizzaTopping ;
        rdfs:subPropertyOf  :hasIngredient ;
        owl:inverseOf       :isToppingOf .

:LeekTopping  a           owl:Class ;
        rdfs:label        "CoberturaDeLeek"@pt ;
        rdfs:subClassOf   :VegetableTopping ;
        rdfs:subClassOf   [ a                   owl:Restriction ;
                            owl:onProperty      :hasSpiciness ;
                            owl:someValuesFrom  :Mild
                          ] ;
        owl:disjointWith  :SpinachTopping , :MushroomTopping , :PepperTopping , :GarlicTopping , :RocketTopping , :AsparagusTopping , :PetitPoisTopping , :OliveTopping , :OnionTopping , :ArtichokeTopping , :CaperTopping , :TomatoTopping .

:HamTopping  a            owl:Class ;
        rdfs:label        "CoberturaDePresunto"@pt ;
        rdfs:subClassOf   :MeatTopping ;
        owl:disjointWith  :PeperoniSausageTopping , :ChickenTopping , :HotSpicedBeefTopping .

:PeperoniSausageTopping
        a                 owl:Class ;
        rdfs:label        "CoberturaDeCalabreza"@pt ;
